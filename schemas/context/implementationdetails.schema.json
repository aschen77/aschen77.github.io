            "meta:xdmField": "xdm:name",
            "tagNumber": 1,
            "enumValues": [
                {
                    "scode": {
                        "title": "Adobe Analytics sCode",
                        "tagNumber": 0
                    }
                },
                {
                    "appmeasurement": {
                        "title": "Adobe App Measurement",
                        "tagNumber": 1
                    }
                },
                {
                    "mbox": {
                        "title": "Adobe Target mBox",
                        "tagNumber": 2
                    }
                },
                {
                    "visitorapi": {
                        "title": "Adobe Visitor Service",
                        "tagNumber": 3
                    }
                },
                {
                    "adcloud/viewability": {
                        "title": "Adobe AdCloud Viewability",
                        "tagNumber": 4
                    }
                }
            ]
        },
        "version": {
            "title": "Version",
            "type": "string",
            "description": "The version identifier of the API, e.g h.18.",
            "meta:xdmType": "string",
            "meta:xdmField": "xdm:version",
            "tagNumber": 2
        }
    },
    "meta:xdmId": "https://ns.adobe.com/xdm/context/implementationdetails",
    "meta:altId": "_xdm.context.implementationdetails"
